# Admin app environment contract
# Copy to .env.local for local development.

# Firebase client configuration
ADMIN_FIREBASE_API_KEY=
ADMIN_FIREBASE_AUTH_DOMAIN=
ADMIN_FIREBASE_PROJECT_ID=
ADMIN_FIREBASE_STORAGE_BUCKET=
ADMIN_FIREBASE_MESSAGING_SENDER_ID=
ADMIN_FIREBASE_APP_ID=
ADMIN_FIREBASE_MEASUREMENT_ID=

# Release environment selection
ADMIN_ENVIRONMENT=dev

# Canonical Firebase project IDs
ADMIN_PROJECT_ID_DEV=
ADMIN_PROJECT_ID_STAGING=
ADMIN_PROJECT_ID_PROD=

# Feature flags
ADMIN_ENABLE_SERVICE_REQUESTS=true
ADMIN_ENABLE_ESCORT_OPTION=false
