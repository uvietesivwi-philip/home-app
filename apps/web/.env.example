# Web app environment contract
# Copy to .env.local for local development.

# Firebase client configuration
WEB_FIREBASE_API_KEY=
WEB_FIREBASE_AUTH_DOMAIN=
WEB_FIREBASE_PROJECT_ID=
WEB_FIREBASE_STORAGE_BUCKET=
WEB_FIREBASE_MESSAGING_SENDER_ID=
WEB_FIREBASE_APP_ID=
WEB_FIREBASE_MEASUREMENT_ID=

# Release environment selection
WEB_ENVIRONMENT=dev

# Canonical Firebase project IDs
WEB_PROJECT_ID_DEV=
WEB_PROJECT_ID_STAGING=
WEB_PROJECT_ID_PROD=

# Feature flags
WEB_ENABLE_SERVICE_REQUESTS=true
WEB_ENABLE_ESCORT_OPTION=false
