# Mobile app environment contract
# Copy to .env.local for local development.

# Firebase client configuration
MOBILE_FIREBASE_API_KEY=
MOBILE_FIREBASE_AUTH_DOMAIN=
MOBILE_FIREBASE_PROJECT_ID=
MOBILE_FIREBASE_STORAGE_BUCKET=
MOBILE_FIREBASE_MESSAGING_SENDER_ID=
MOBILE_FIREBASE_APP_ID=
MOBILE_FIREBASE_MEASUREMENT_ID=

# Release environment selection
MOBILE_ENVIRONMENT=dev

# Canonical Firebase project IDs
MOBILE_PROJECT_ID_DEV=
MOBILE_PROJECT_ID_STAGING=
MOBILE_PROJECT_ID_PROD=

# Feature flags
MOBILE_ENABLE_SERVICE_REQUESTS=true
MOBILE_ENABLE_ESCORT_OPTION=false
